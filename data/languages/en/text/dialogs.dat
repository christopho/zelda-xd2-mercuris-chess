  id = "lafoo_riot.npc_46",
  skip = "all",
  translators_note = "Reference to Gandalf in The Lord of the Rings",
  text = [[
YOU.


SHALL.


NOT.


PASS!!!
]],
}

dialog{
  id = "lafoo_riot.npc_47",
  skip = "all",
  text = [[
