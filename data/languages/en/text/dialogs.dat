]],
}

dialog{
  id = "lafoo_riot.npc_2",
  skip = "all",
  translators_note = "Reference to left-wing French politician Jean-Luc Melenchon, famous for his bombastic speeches",
  text = [[
YES SIR!
DON'T BE AFRAID
OF WORDS!!!
This building on
fire is a symbolic
thing!
This expresses the
