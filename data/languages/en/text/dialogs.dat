dialog{
  id = "main_village.vase_house",
  skip = "all",
  translators_note = "Reference to Zelda games where you always break all the pots",
  text = [[
There has been some
vandalism. A person
in green came here
and broke all pots!
People don't respect
things anymore.
I've been cleaning this
house for two weeks.
I've taken measures
to prevent that again.
Mwahahaha!!!
]],
}

dialog{
  id = "main_village.villager_tunnels",
  skip = "all",
  translators_note = "Reference to the game Minecraft",
  text = [[
